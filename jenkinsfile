pipeline {
    agent any

    stages {
        stage('Clone Repository') {
            steps {
                git 'git@github.com:DEL-ORG/Projeckt-do-it-now-Pipeline.git'
            }
        }
        stage('Maven Test') {
            steps {
                sh'''
                pwd
                mvn clean
                mvn package

                '''
            }
        }
    }
}
