pipeline {
    agent any

    stages {
        stage('Clone Repository') {
            steps {
                git branch: 'main', url: 'https://github.com/DEL-ORG/Projeckt-do-it-now-Pipeline.git'
            }
        }
        stage('Maven Test') {
            steps {
                sh 'mvn test'
            }
        }
    }
}
